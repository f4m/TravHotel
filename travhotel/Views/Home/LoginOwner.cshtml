
@{
    ViewBag.Title = "OwnerLogin";
}

<body>
    <center>
        <h1>Place Your Hotels and Restaurants</h1>
        <form action="" method="post">
            <table>
                <tr>
                    <td>Email:</td>
                    <td>
                        <input type="text" name="owner_email">
                    </td>
                </tr>
                <tr>
                    <td>Password:</td>
                    <td>
                        <input type="Password" name="owner_pass">
                    </td>
                </tr>
                <tr>
                    <td>
                        <input type="submit" name="owner_login" value="Login">
                    </td>
                    <td>
                        Not have an account yet? @Html.ActionLink("Register","RegisterOwner","Home")
                    </td>
                </tr>
            </table>

        </form>
    </center>
</body>

