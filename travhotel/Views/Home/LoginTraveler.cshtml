
@{
    ViewBag.Title = "LoginTraveler";
}

<body>
    <center>
        <h1>Welcome Traveler</h1>
        <form action="" method="post">
            <table>
                <tr>
                    <td>Email:</td>
                    <td>
                        <input type="text" name="traveler_email">
                    </td>
                </tr>
                <tr>
                    <td>Password:</td>
                    <td>
                        <input type="Password" name="traveler_pass">
                    </td>
                </tr>
                <tr>
                    <td>
                        <input type="submit" name="traveler_login" value="Login">
                    </td>
                    <td>
                        Not a travler yet? @Html.ActionLink("Register","RegisterTraveler","home")
                    </td>
                </tr>
            </table>

        </form>
    </center>
</body>

